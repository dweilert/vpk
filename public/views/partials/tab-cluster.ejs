            <span class="nodeMemory">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterNodeMemory">
                Node Memory
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterNodeCPU"
                name="clusterFilterNodeCPU">
            <span class="nodeCPU">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterNodeCPU">
                Node CPU
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterNodeStorage"
                name="clusterFilterNodeStorage">
            <span class="nodeStorage">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterNodeStorage">
                Node Persistent Storage
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterMemoryLimit"
                name="clusterFilterMemoryLimit">
            <span class="memoryLimit">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterMemoryLimit">
                Pod Memory Limit
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterMemoryRequest"
                name="clusterFilterMemoryRequest">
            <span class="memoryRequest">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterMemoryRequest">
                Pod Memory Requested
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterCPULimit"
                name="clusterFilterCPULimit">
            <span class="cpuLimit">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterCPULimit">
                Pod CPU Limit
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterCPURequest"
                name="clusterFilterCPURequest">
            <span class="cpuRequest">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterCPURequest">
                Pod CPU Requested
            </label>
        </div>

        <hr style="margin-left: -10px;">
        <div class="form-check vpkfont-filter">
            <span>NAMESPACES</span>&nbsp;&nbsp;
            <span class="vpkfont-filter">(default: all-namespaces)</span>
        </div>

        <div id="cluster-bar1-select" class="form-check mr-1 mt-1 vpkfont-filter">
            <select id="cluster-ns-filter" multiple="multiple" style="width: 250px" id="cluster-bar1-options">
                <option>all-namespaces</option>
                <option>&lt;cluster-level&gt;</option>
                <option>default</option>
            </select>
        </div>

        <hr style="margin-left: -10px;">
        <div class="form-check vpkfont-filter">
            <span>SOUND</span>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterSound" checked>
            <label class="form-check-label" for="clusterFilterSound">
                Play Sound
                <span class="vpkfont-filter">(sound indicates resource was selected in view)</span>
            </label>
        </div>

        <hr style="margin-left: -10px;">
        <div class="mb-2 ml-3">
            <button type="button" id="c3DBackground" class="btn btn-primary btn-sm vpkButtons pd-2"
                onclick="saveClusterFilters()">Save filters as default</button>
        </div>
