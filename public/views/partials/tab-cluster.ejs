<!-- Tab: CLUSTER  -->
<div id="cluster" class="tab-pane">
    <!-- Cluster 3D View-->
    <div id="cluster3DView" style="display: none;">
        <div class="cluster-content">
            <canvas id="clusterCanvas"></canvas>
        </div>
    </div>

    <span id="slideIn_box">Properties</span>
    <div id="slideIn" class="hidden">
        <button type="button" class="btn btn-outline-primary vpkButtons vpkwhite header-right mt-1 mr-1"
            onclick="closeSlideIn()">&nbsp;Close&nbsp;
        </button>

        <div class="content">
            <div class="mr-2 pd-2">
                <!-- <div id="resourceProps" class="mt-1 mb-1 mr-1 pr-3 pt-3 pb-3 vpkfont vpkcolor"> -->
                <div id="resourceProps" class="mt-1 mb-1 mr-1 vpkfont vpkcolor">
                    <!-- Resource info inserted here -->
                </div>
            </div>
        </div>
    </div>

    <!-- FILTER on left of screen-->
    <span id="cluster_filter">Filter view</span>
    <div id="cluster_filter_box">
        <div style="display: inline;">
            <div class="form-check vpkfont-filter">
                <span class="vpkcolor align-middle">Resource Categories</span>
                <button type="button" class="btn btn-outline-primary vpkButtons vpkwhite header-right"
                    onclick="closeClusterFilter()">&nbsp;Close&nbsp;
                </button>
            </div>
        </div>
        <hr>
        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterNodes"
                name="clusterFilterNodes" checked>
            <span class="nodeMst">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>&nbsp;<span
                class="nodeWrk">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterNodes">
                Nodes (Master, Worker)
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterCSI"
                name="clusterFilterCSI" checked>
            <span class="csiStorage">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterCSI">
                CSI (Container Storage Interface)
            </label>
        </div>


        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterStorage"
                name="clusterFilterStorage" checked>
            <span class="pvc">&nbsp;&nbsp;&nbsp;</span>&nbsp;<span class="pv">&nbsp;&nbsp;&nbsp;</span>&nbsp;<span
                class="storageClass">&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterStorage">
                Storage (PVC, PV, Storage Class)
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterNetwork"
                name="clusterFilterNetwork" checked>
            <span class="endpoint">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>&nbsp;<span
                class="service">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterNetwork">
                Network (Endpoint, Service)
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterIngress"
                name="clusterFilterIngress" checked>
            <span class="ingress">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterIngress">
                Ingress/IngressControl
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterControlP"
                name="clusterFilterControlP" checked>
            <span class="controlPlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterControlP">
                Control Plane
            </label>
        </div>


        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterWorkload"
                name="clusterFilterWorkload" checked>
            <span class="workloadPlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterWorkload">
                Workload level plane
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterService"
                name="clusterFilterService" checked>
            <span class="servicePlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterService">
                Service level plane
            </label>
        </div>


        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterConfigStorage"
                name="clusterFilterConfigStorage" checked>
            <span class="configStoragePlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterConfigStorage">
                ConfigStorage level plane
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterAuthentication"
                name="clusterFilterAuthentication" checked>
            <span class="authenticationPlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterAuthentication">
                Authenticate related plane
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterAuthorization"
                name="clusterFilterAuthorization" checked>
            <span class="authorizationPlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterAuthorization">
                Authorizaiton related plane
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterPolicy"
                name="clusterFilterPolicy" checked>
            <span class="policyPlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterPolicy">
                Policy related plane
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterExtend"
                name="clusterFilterExtend" checked>
            <span class="extendPlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterExtend">
                Extend resources plane
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterCluster"
                name="clusterFilterCluster" checked>
            <span class="clusterPlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterCluster">
                Cluster resources plane
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterOther"
                name="clusterFilterOther" checked>
            <span class="otherPlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterOther">
                Other resources plane
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1 vpk-vert-mid">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterThirdParty"
                name="clusterFilterThirdParty" checked>
            <span class="thirdPartyPlane">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterThirdParty">
                ThirdParty resources plane
            </label>
        </div>



        <hr style="margin-left: -10px;">
        <div class="form-check vpkfont-filter">
            <span>Pods</span>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterRunning"
                name="clusterFilterRunning" checked>
            <span class="podRunning">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterRunning">
                Running Pods
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterWarning"
