            <span class="podWarn">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterWarning">
                Warning Pods
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterFailed"
                name="clusterFilterFailed" checked>
            <span class="podFail">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterFailed">
                Failed Pods
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterSuccessful"
                name="clusterFilterSuccessful" checked>
            <span class="podSuccessful">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterSuccessful">
                Successful/Finished Pods
            </label>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterDSPods"
                name="clusterFilterDSPods">
            <span class="podDaemon">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <label class="form-check-label" for="clusterFilterDSPods">
                DaemonSet Pods
            </label>
        </div>

        <hr style="margin-left: -10px;">
        <div class="form-check vpkfont-filter">
            <span>Resource CPU & Memory</span>
        </div>

        <div class="form-check vpkfont-filter ml-1">
            <input class="form-check-input filter-checkbox" type="checkbox" value="" id="clusterFilterNodeMemory"
