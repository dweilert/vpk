{
    "defaults": {
        "managedFields": false,
        "statusSection": true
    },
    "snapshotDir": "/Users/bob/snaptest",
    "xrefNames": {
        "secrets": {
            "xon": true,
            "xtype": "s",
            "desc": "Secrets defined in the environment"
        },
        "configs": {
            "xon": true,
            "xtype": "s",
            "desc": "ConfigMaps defined in the environment"
        },
        "podUids": {
            "xon": true,
            "xtype": "s",
            "desc": "UID of each pod"
        },
        "labelsApp": {
            "xon": false,
            "xtype": "s",
            "desc": "Labels for app prometheus"
        },
        "routeHosts": {
            "xon": true,
            "xtype": "s",
            "desc": "host on route"
        }
    },
    "xrefRules": {
        "Pod": [
            {
                "xon": true,
                "xtype": "s",
                "xid": "x0",
                "xrk": "configs",
                "xrw": "spec.containers[*].env[*].valueFrom.configMapKeyRef.name"
            },
            {
                "xon": true,
                "xtype": "s",
                "xid": "1",
                "xrk": "configs",
                "xrw": "spec.volumes[*].configMap[*].name"
            },
            {
                "xon": true,
                "xtype": "s",
                "xid": "x2",
                "xrk": "podUids",
                "xrw": "metadata.uid"
            },
            {
                "xon": true,
                "xtype": "s",
                "xid": "x3",
                "xrk": "secrets",
                "xrw": "spec.volumes[*].secret.secretName"
            },
            {
                "xon": true,
                "xtype": "s",
                "xid": "x4",
                "xrk": "labelsApp",
                "xrw": "metadata.labels.app{{prometheus}}"
            }
        ],
        "ServiceAccount": [
            {
                "xon": true,
                "xtype": "s",
                "xid": "x0",
                "xrk": "secrets",
                "xrw": "secrets[*].name"
            },
            {
                "xon": true,
                "xtype": "s",
                "xid": "x1",
                "xrk": "secrets",
                "xrw": "imagePullSecrets[*].name"
            }
        ],
        "Route": [
            {
                "xon": true,
                "xtype": "s",
                "xid": "x0",
                "xrk": "routeHosts",
                "xrw": "spec.host"
            }
        ]
    }
}